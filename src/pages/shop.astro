---
import Head from "../components/Head.astro";
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";
---

<html>
  <Head
    title="Tire Calligraphy - Shop"
    url="https://tirecalli.com/shop"
    description="199144 and New Life are a Tire Calligraphy ongoing experimental projects. We experiment with material, color, shapes and cuts. Every item is a neutral gender item."
  />
  <body class="h-100 one-nine-nine-four-four">
    <Header />
    <main>
      <section class="shop-section">
        <img class="shop-img aspect-ratio aspect-ratio-revert" src="/img/heart-coeurt.webp" alt="tirecalli-picture" />
      </section>
      <section class="cards">
        <a href="/Nude-Pink-Licitar-T-Shirt/">
          <article>
            <div class="card__image">
              <img src="/img/Nude-Pink-Licitar-T-Shirt/man-in-nude-pink-licitar-t-shirt-back.webp" alt="tirecalli-picture" />
            </div>
            <h5>Nude Pink Licitar T-Shirt</h5>
            <div class="card__description">
              <p>70€</p>
            </div>
          </article>
        </a>
        <a href="/Sand-Brown-Licitar-T-Shirt/">
          <article>
            <div class="card__image">
              <img src="/img/Sand-Brown-Licitar-T-Shirt/girl-in-sand-brown-licitar-t-shirt-back.webp" alt="tirecalli-picture" />
            </div>
            <h5>Sand Brown Licitar T-Shirt</h5>
            <div class="card__description">
              <p>70€</p>
            </div>
          </article>
        </a>
        <a href="/Pale-Lemon-Licitar-T-Shirt/">
          <article>
            <div class="card__image">
              <img src="/img/Pale-Lemon-Licitar-T-shirt/man-in-pale-lemon-licitar-t-shirt-back.webp" alt="tirecalli-picture" />
            </div>
            <h5>Pale Lemon Licitar T-Shirt</h5>
            <div class="card__description">
              <p>60€</p>
            </div>
          </article>
        </a>
        <!-- <a href="/Pale-Lemon-Licitar-Tank-Top/"> -->
        <article>
          <div class="card__image">
            <img src="/img/Pale-Lemon-Licitar-T-shirt/girl-in-pale-lemon-licitar-t-shirt-back.webp" alt="tirecalli-picture" />
          </div>
          <h5>Pale Lemon Licitar Tank Top</h5>
          <div class="card__description">
            <!-- <p>60€</p> -->
            <p>SOON</p>
          </div>
        </article>
        <!-- </a> -->
        <a href="/Ash-Brown-Licitar-Longsleeve-T/">
          <article>
            <div class="card__image">
              <img src="/img/Ash-Brown-Licitar-T-Shirt/girl-in-ash-brown-licitar-longsleeve-t-shirt-side-detail.webp" alt="tirecalli-picture" />
            </div>
            <h5>Ash Brown Licitar Longsleeve T</h5>
            <div class="card__description">
              <p>75€</p>
            </div>
          </article>
        </a>
      </section>
      <section class="shop-section">
        <img class="shop-img aspect-ratio aspect-ratio-revert" src="/img/crypto-kids-cover.webp" alt="tirecalli-picture" />
        <div class="shop-text">
          <h4>CRYPTO KIDS</h4>
        </div>
      </section>
      <section class="cards">
        <a href="/Hybrid-Jacket/">
          <article>
            <div class="card__image">
              <img src="/img/Hybrid-Jacket/girl-in-hybrid-jacket-3.webp" alt="tirecalli-picture" />
            </div>
            <h5>Hybrid Jacket</h5>
            <div class="card__description">
              <p>250€</p>
            </div>
          </article>
        </a>
        <a href="/Basic-Denim-Jacket/">
          <article>
            <div class="card__image">
              <img src="/img/Basic-Denim-Jacket/man-in-basic-denim-jacket.webp" alt="tirecalli-picture" />
            </div>
            <h5>Basic Denim Jacket</h5>
            <div class="card__description">
              <p>180€</p>
            </div>
          </article>
        </a>
        <a href="/Black-Quilted-Jacket/">
          <article>
            <div class="card__image">
              <img src="/img/Black-Quilted-Jacket/man-in-black-quilted-jacket-front.webp" alt="tirecalli-picture" />
            </div>
            <h5>Black Quilted Jacket</h5>
            <div class="card__description">
              <p>80€</p>
            </div>
          </article>
        </a>
        <a href="/Basic-Denim-Overall-Pant/">
          <article>
            <div class="card__image">
              <img src="/img/Basic-Denim-Overall-Pant/girl-in-basic-denim-overall-pant-2.webp" alt="tirecalli-picture" />
            </div>
            <h5>Basic Denim Overall Pant</h5>
            <div class="card__description">
              <p>SOLD OUT</p>
              <p>Preorder available</p>
            </div>
          </article>
        </a>
      </section>
      <section class="shop-section">
        <img class="shop-img aspect-ratio aspect-ratio-revert" src="/img/new-life-cover.webp" alt="tirecalli-picture" />
        <div class="shop-text">
          <h4>NEW LIFE</h4>
        </div>
      </section>
      <section class="cards">
        <a href="/Black-TC-Tote-bag/">
          <article>
            <div class="card__image">
              <img src="/img/Black-TC-Tote-bag/girl-in-grey-tshirt-with-tote-bag-and-laptop.webp" alt="tirecalli-picture" />
            </div>
            <h5>Black TC Tote bag</h5>
            <div class="card__description">
              <p>50€</p>
            </div>
          </article>
        </a>
        <a href="/New-Life-Gray-T-Shirt/">
          <article>
            <div class="card__image">
              <img src="/img/New-Life-Gray-T-Shirt/gray-tshirt-new-life-mens-2.webp" alt="tirecalli-picture" />
            </div>
            <h5>New Life Gray T-Shirt</h5>
            <div class="card__description">
              <p>SOLD OUT</p>
            </div>
          </article>
        </a>
        <a href="/Dobokh-Parkha/">
          <article>
            <div class="card__image">
              <img transition:name="test" src="/img/Dobokh-Parkha/girl-in-black-jacket-front.webp" alt="tirecalli-picture" />
            </div>
            <h5>Dobokh Parkha</h5>
            <div class="card__description">
              <p>SOLD OUT</p>
            </div>
          </article>
        </a>
      </section>
      <section class="cards">
        <a href="/Dobokh-Pant/">
          <article>
            <div class="card__image">
              <img src="/img/Dobokh-Pant/womens-black-pant-with-belt-in-a-knot.webp" alt="tirecalli-picture" />
            </div>
            <h5>Dobokh Pant</h5>
            <div class="card__description">
              <p>SOLD OUT</p>
            </div>
          </article>
        </a>
        <a href="/New-Life-Black-T-Shirt/">
          <article>
            <div class="card__image">
              <img src="/img/New-Life-Black-T-Shirt/man-in-new-life-black-tshirt-front.webp" alt="tirecalli-picture" />
            </div>
            <h5>New Life Black T-Shirt</h5>
            <div class="card__description">
              <p>SOLD OUT</p>
            </div>
          </article>
        </a>
        <a href="/New-Life-Black-Hoodie/">
          <article>
            <div class="card__image">
              <img src="/img/New-Life-Black-Hoodie/girl-in-black-new-life-hoodie-front-cover.webp" alt="tirecalli-picture" />
            </div>
            <h5>New Life Black Hoodie</h5>
            <div class="card__description">
              <p>SOLD OUT</p>
            </div>
          </article>
        </a>
      </section>
    </main>
    <Footer />
    <script src="../js/app.js"></script>
  </body>
</html>
